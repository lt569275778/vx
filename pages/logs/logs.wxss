/* pages/logs/logs.wxss */
.outer {
    background-color: #f8fbff;
    border-top: 1px solid #faa47a;
    border-bottom: 1px solid #f8fbff; 
}
.logs_top {
    background: linear-gradient(#faa47a, #f46859);
}
.doubt {
    height: 50rpx;
    text-align: right;
    padding-right: 24rpx;
}
.doubt image {
    width: 140rpx;
    height: 80%;
}
.excircle {
    border: 1px solid #f8b8a0;
    width: 280rpx;
    height: 280rpx;
    line-height: 280rpx;
    margin: 0 auto;
    border-radius: 50%;
    background-color: #f8bba4;
}
.fillet {
    width: 220rpx;
    height: 220rpx;
    line-height: 220rpx;
    margin: 28rpx 0 0 28rpx;
    border-radius: 50%;
    background-color: #fff;
    box-shadow: 0rpx 0rpx 20rpx #f84b10;
    text-align: center;
    border: 1px solid #fff;
}
.grade_til {
    margin-top: 10rpx;
    font-size: 28rpx;
    color: #353535;
    display: block;
    height: 20rpx;
    line-height: 20rpx;

         /* border:1px solid red;  */
}
.grade {
    display: block;
    margin-top: 50rpx;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 51rpx;
    color: #fb7a12;

         /* border:1px solid green;  */
}
.logs_list {
    margin-top: 50rpx;
}
.logs_list view {
    width: 24.5%;
    display: inline-block;
    font-size: 30rpx;
    text-align: center;
    color: #fff;
}
.logs_list text {
    display: block;
}
.logs_list view text:nth-child(2) {
    margin-top: 20rpx;
}
.logs_list view:not(:nth-child(1)) {
    border-left: 2rpx solid #fcbeb2;
}
.sahde {
    height: 124rpx;
}
.sahde image {
    width: 100%;
    height: 100%;
}
.logs_main {
    margin: 52rpx 0 2rpx 0;
    text-align: center;
    font-size: 36rpx;
    color: #353535;
}
.btn_L {
    width: 300rpx;
    height: 100rpx;
    display: inline-block;
    border-top: 2rpx solid #f5936a;
    border-left: 2rpx solid #f5936a;
    border-bottom: 2rpx solid #f5936a;
    line-height: 100rpx;
    border-radius: 50rpx 0 0 50rpx;
}
.btn_R {
    width: 300rpx;
    height: 100rpx;
    display: inline-block;
    line-height: 100rpx;
    border-top: 2rpx solid #f5936a;
    border-right: 2rpx solid #f5936a;
    border-bottom: 2rpx solid #f5936a;
    border-radius: 0 50rpx 50rpx 0;
}
.on {
    background: linear-gradient(#f5936a, #f56c56);
    color: #fff;
}
.myModule {
    padding: 0rpx 30rpx;
    height: 150rpx;
    line-height: 150rpx;
    background-color: #fff;
   box-shadow:0rpx 0rpx 15rpx 5rpx #d8e6ff;
   margin-top: 30rpx;       
}
 .shadow_up{
    height: 2rpx;
    width: 100%;
    background-color: #fff;
    box-shadow:0rpx 0rpx 15rpx 4rpx #d8e6ff;
    margin-bottom: 40rpx;
}
.myLog {
    margin: 0 46rpx 0 16rpx;
    vertical-align: top;
    font-size: 36rpx;
    color: #353535;
    float: left;
}
.myModule view {
    height: 108rpx;
    width: 108rpx;
    margin-top: 21rpx;
    float: left;
}
.myModule view image {
    width: 100rpx;
    height: 100rpx;
    border: 4rpx solid #fcd2bd;
    border-radius: 50%;
}
.myName {
    margin-left: 24rpx;
    vertical-align: top;
    font-size: 30rpx;
    color: #353535;
    float: left;

        width: 150rpx;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
        text-align: left;
}
.myNumber {
    float: right;
    vertical-align: top;
    font-size: 36rpx;
    color: #fa876a;

}
.foot {
    margin: 46rpx 0 150rpx;
    border-top: 1rpx solid #e0ebfb;
    border-bottom: 1rpx solid #e0ebfb;
    box-shadow:0rpx 0rpx 15rpx 6rpx #d8e6ff;
    margin-bottom: 60rpx;
}
.first_myModule {
    padding: 0rpx 30rpx;
    height: 150rpx;
    line-height: 150rpx;
    background-color: #fff;
}
.first_log {
    margin-top: 46rpx;
    width: 58rpx;
    height: 58rpx;
    float: left;
}
.first_log image {
    width: 50rpx;
    height: 50rpx;
    border: 4rpx solid #fcd2bd;
    border-radius: 50%;
    vertical-align: top;
}
.first_head {
    margin: 21rpx 24rpx 0rpx 28rpx;
    width: 108rpx;
    height: 108rpx;
    float: left;
}
.first_head image {
    width: 100rpx;
    height: 100rpx;
    border: 4rpx solid #fcd2bd;
    border-radius: 50%;
}
.first_myName {
    vertical-align: top;
    font-size: 30rpx;
    color: #353535;
    float: left;
    width: 240rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    text-align: left;
}
.first_myNumber {
    float: right;
    font-size: 36rpx;
    color: #fa876a;
}
.con_Log {
    margin: 0 14rpx 0 8rpx;
    vertical-align: top;
    font-size: 36rpx;
    color: #353535;
    float: left;
    width: 40rpx;
}
.swiper-box {
     margin-top: 42rpx; 
    margin-bottom: 0rpx;
    height: 1820rpx !important;     
}

.c1{
    padding: 30rpx 0 0 78%;
}